<script>
    // Đoạn mã sẽ chạy khi trang được tải hoàn thành
    $(document).ready(function() {
        // Lấy giá trị của input hidden chứa id sinh viên
        var studentId = $("#studentInput").val();

        // Gọi hàm để đếm số thông báo
        countNotifications(studentId);
    });

    // Hàm để đếm số thông báo
    function countNotifications(studentId) {
        // Gửi yêu cầu đến server để lấy số thông báo dựa trên logic của bạn
        // Ở đây là một ví dụ cứng, bạn cần thay thế nó với logic thực tế của mình
        var numberOfNotifications = getNumberOfNotifications(studentId);

        // Cập nhật số lượng thông báo trên giao diện
        updateNotificationCount(numberOfNotifications);
    }

    // Hàm để gửi yêu cầu đến server và nhận số thông báo
    function getNumberOfNotifications(studentId) {
        // Thực hiện yêu cầu Ajax hoặc fetch đến server để lấy số thông báo
        // Ở đây là một ví dụ cứng, bạn cần thay thế nó với logic thực tế của mình
        // Đảm bảo rằng bạn trả về số thông báo từ server
        return 3; // Ví dụ: Trả về số 3, bạn cần thay đổi nó với logic thực tế
    }

    // Hàm để cập nhật số lượng thông báo trên giao diện
    function updateNotificationCount(count) {
        // Chọn thẻ badge và cập nhật với số mới
        $("#notificationBadge").text(count);

        // Nếu số thông báo là 0, bạn có thể ẩn badge hoặc thực hiện xử lý khác
        if (count === 0) {
            $("#notificationBadge").hide();
        } else {
            $("#notificationBadge").show();
        }
    }
</script>

