<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.17.2/xlsx.full.min.js"></script>
<script>
    document.getElementById('excelFile').addEventListener('change', handleFile);

    function handleFile(e) {
        var file = e.target.files[0];
        if (!file) return;

        var reader = new FileReader();
        reader.onload = function (e) {
            var data = e.target.result;
            var workbook = XLSX.read(data, { type: 'binary' });

            // Xử lý mã lớp và môn học
            handleClassCodeAndSubject(workbook);

            // Xử lý thông tin sinh viên và bảng
            handleStudentInfoTable(workbook);
        };

        reader.readAsBinaryString(file);
    }

    function handleClassCodeAndSubject(workbook) {
        // Lấy dữ liệu từ các ô cần thiết
        var semesterCell = workbook.Sheets[workbook.SheetNames[0]]['F4'];
        var subjectCell = workbook.Sheets[workbook.SheetNames[0]]['C5'];
        var groupCell = workbook.Sheets[workbook.SheetNames[0]]['J5'];

        // Tạo mã lớp
        var year = new Date().getFullYear().toString().substr(-2); // Lấy hai chữ số cuối cùng của năm hiện tại
        var semester = semesterCell ? semesterCell.v.match(/\d+/)[0] : '';
        var subjectCode = subjectCell ? subjectCell.v.match(/\(([^)]+)\)/)[1] : '';
        var groupCode = groupCell ? groupCell.v : '';
        var classCode = year + semester + '.' + subjectCode + '.' + groupCode;

        // Đặt giá trị vào ô mã lớp
        document.getElementById('exampleInputText02').value = classCode;

        // Lấy giá trị từ ô A1 để đặt vào ô môn học
        var cellValue = workbook.Sheets[workbook.SheetNames[0]]['C5'].v;
        // Đặt giá trị vào ô môn học
        document.getElementById('exampleInputText01').value = cellValue;
    }
    function updateSTT() {
    var table = document.getElementById('user-list-table');
    var rows = table.rows;

    for (var i = 1; i < rows.length; i++) {
        rows[i].cells[0].innerText = i;
    }
}
    function handleStudentInfoTable(workbook) {
        // Lấy dữ liệu từ sheet đầu tiên
        var sheet = workbook.Sheets[workbook.SheetNames[0]];

        // Lấy đề mục
        var headers = ["STT", "MSSV", "Họ và tên", "Ngày sinh"];

        // Xóa dữ liệu cũ trong bảng
        var table = document.getElementById('user-list-table');
        var tbody = table.getElementsByTagName('tbody')[0];
        tbody.innerHTML = '';

        // Đọc dữ liệu từ file Excel và điền vào bảng
        var row = 9; // Bắt đầu từ dòng chứa đề mục
        var rowData;

        do {
            rowData = [];
            headers.forEach(function (header) {
                var cell = sheet[header + row];
                rowData.push(cell ? cell.v : ''); // Lấy giá trị từ ô hoặc để trống nếu ô trống
            });

            console.log("rowData:", rowData); // In dữ liệu hàng ra console để kiểm tra

            // Thêm dòng vào bảng
            var newRow = tbody.insertRow();
            rowData.forEach(function (value, index) {
                var cell = newRow.insertCell(index);
                cell.innerHTML = value;
            });

            row++;
        } while (rowData.some(Boolean)); // Kiểm tra xem có dữ liệu trong dòng không

        // Hiển thị số lượng sinh viên
        var infoDiv = document.getElementById('user_list_datatable_info');
        infoDiv.innerHTML = "Hiển thị " + (row - 10) + " sinh viên"; // Trừ đi dòng chứa đề mục

        // Tính số thứ tự (STT)
        updateSTT();
    }

    // Định nghĩa hàm xử lý sự kiện khi chọn file
    document.getElementById('excelFile').addEventListener('change', handleFile);

    // Định nghĩa hàm chính xử lý file
    function handleFile(e) {
        var file = e.target.files[0];
        if (!file) return;

        var reader = new FileReader();
        reader.onload = function (e) {
            var data = e.target.result;
            var workbook = XLSX.read(data, { type: 'binary' });

            // Xử lý mã lớp và môn học
            handleClassCodeAndSubject(workbook);

            // Xử lý thông tin sinh viên và bảng
            handleStudentInfoTable(workbook);
        };

        reader.readAsBinaryString(file);
    }
    </script>




